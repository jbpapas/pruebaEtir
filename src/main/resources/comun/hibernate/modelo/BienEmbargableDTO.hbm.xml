<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="es.dipucadiz.etir.comun.dto.BienEmbargableDTO" table="GA_BIEN_EMBARGABLE">
        <id name="coBienEmbargable" type="java.lang.Long">
            <column name="CO_BIEN_EMBARGABLE" precision="10" scale="0" />
            <generator class="sequence">
                <param name="sequence">SEQ_BIEN_EMBARGABLE</param>
            </generator>
        </id>
    
        <many-to-one name="clienteDTO" class="es.dipucadiz.etir.comun.dto.ClienteDTO" fetch="select">
            <column name="CO_CLIENTE" length="10" />
        </many-to-one>
  
        <property name="tipo" type="string">
            <column name="TIPO" length="2" />
        </property>

        <property name="subTipo" type="string">
            <column name="SUBTIPO" length="2" />
        </property>        

        <property name="identificacion" type="string">
            <column name="IDENTIFICACION" length="200" />
        </property> 

       <many-to-one name="documentoOrigenDTO" class="es.dipucadiz.etir.comun.dto.DocumentoDTO" fetch="select">
	        <column name="CO_MODELO_ORIGEN" length="3" />
	        <column name="CO_VERSION_ORIGEN" length="1" />
	        <column name="CO_DOCUMENTO_ORIGEN" length="9" />
        </many-to-one>
 
        <many-to-one name="documentoBienDTO" class="es.dipucadiz.etir.comun.dto.DocumentoDTO" fetch="select">
	        <column name="CO_MODELO_BIEN" length="3" />
	        <column name="CO_VERSION_BIEN" length="1" />
	        <column name="CO_DOCUMENTO_BIEN" length="9" />
        </many-to-one>
        
        <property name="valorBien" type="big_decimal">
            <column name="IM_VALOR_BIEN" precision="17" />
        </property>         
        
         <property name="fxValoracion" type="date">
            <column name="FX_VALORACION" length="7" />
        </property>
       
        <property name="estado" type="string">
            <column name="ESTADO" length="2" />
        </property>
         
        <property name="observaciones" type="string">
            <column name="OBSERVACIONES" length="200" />
        </property> 
                          
        <many-to-one name="expedienteDTO" class="es.dipucadiz.etir.comun.dto.ExpedienteDTO" fetch="select">
            <column name="CO_EXPEDIENTE" length="10" />
        </many-to-one>        
        
        <property name="fhActualizacion" type="timestamp">
            <column name="FH_ACTUALIZACION" length="11" />
        </property>
        <property name="coUsuarioActualizacion" type="string">
            <column name="CO_USUARIO_ACTUALIZACION" length="10" />
        </property>
        <property name="rowid" type="string" update="false" insert="false">
        	  <column name="ROWID" length="18" />
        </property>
	
	    <many-to-one name="bdDocumentalGrupoDTO" class="es.dipucadiz.etir.comun.dto.BDDocumentalGrupoDTO" fetch="select">
            <column name="CO_BD_DOCUMENTAL_GRUPO" length="6" />
        </many-to-one>
        	
	</class>

</hibernate-mapping>