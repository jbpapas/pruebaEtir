<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="es.dipucadiz.etir.comun.dto.PortafirmasFirmanteDTO" table="GA_PORTAFIRMAS_FIRMANTE">
		<id name="coPortafirmasFirmante" type="java.lang.Long">
            <column name="CO_PORTAFIRMAS_FIRMANTE" precision="10" scale="0" />
            <generator class="sequence">
                <param name="sequence">SEQ_PORTAFIRMAS_FIRMANTE</param>
            </generator>
        </id>
        
        <property name="coCircuito" type="string">
			<column name="CO_CIRCUITO" length="15"/>
		</property>
		
		<property name="fxDesde" type="date">
			<column name="FX_DESDE"/>
		</property>
		
		<property name="fxHasta" type="date">
			<column name="FX_HASTA"/>
		</property>
		
		<property name="nif" type="string">
            <column name="NIF" length="15" />
        </property>
        
        <property name="nombre" type="string">
            <column name="NOMBRE" length="200" />
        </property>
        
        <property name="tipoFirmante" type="string">
            <column name="TIPO_FIRMANTE" length="2" />
        </property>
        
        <property name="fhActualizacion" type="timestamp">
			<column name="FH_ACTUALIZACION" length="6" />
		</property>
        
		<property name="coUsuarioActualizacion" type="string">
			<column name="CO_USUARIO_ACTUALIZACION" length="10" />
		</property>
		
	</class>
</hibernate-mapping>