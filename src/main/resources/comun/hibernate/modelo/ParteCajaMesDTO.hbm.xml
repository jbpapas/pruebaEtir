<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="es.dipucadiz.etir.comun.dto.ParteCajaMesDTO" table="GA_PARTE_CAJA_MES">
        <id name="coParteCajaMes" type="java.lang.Long">
            <column name="CO_PARTE_CAJA_MES" length="10" />
            <generator class="assigned" />
        </id>
        <many-to-one name="ejecucionDTO" class="es.dipucadiz.etir.comun.dto.EjecucionDTO" fetch="select">
            <column name="CO_EJECUCION" precision="10" scale="0" />
        </many-to-one>
        <property name="ano" type="short">
            <column name="AÃ‘O" precision="4" scale="0" not-null="true" />
        </property>
        <property name="mes" type="short">
            <column name="MES" precision="2" scale="0" not-null="true" />
        </property>
        <many-to-one name="municipioDTO" class="es.dipucadiz.etir.comun.dto.MunicipioDTO" fetch="select">
            <column name="CO_PROVINCIA" length="2" not-null="true" />
            <column name="CO_MUNICIPIO" length="3" not-null="true" />
        </many-to-one>
       <many-to-one name="otrDTO" class="es.dipucadiz.etir.comun.dto.OtrDTO" fetch="select">
            <column name="CO_OTR" length="3" />
        </many-to-one>
        <many-to-one name="estadoSituacionDTO" class="es.dipucadiz.etir.comun.dto.EstadoSituacionDTO" fetch="select">
            <column name="CO_ESTADO_SITUACION" length="1" />
        </many-to-one>
        <many-to-one name="modeloDTO" class="es.dipucadiz.etir.comun.dto.ModeloDTO" fetch="select">
            <column name="CO_MODELO" length="3" />
        </many-to-one>
        <property name="ejercicio" type="java.lang.Short">
            <column name="EJERCICIO" precision="4" scale="0" />
        </property>
        <property name="periodo" type="string">
            <column name="PERIODO" length="2" />
        </property>
        <property name="imPrincipal" type="big_decimal">
            <column name="IM_PRINCIPAL" precision="17" />
        </property>
        <property name="imRecApremio" type="big_decimal">
            <column name="IM_REC_APREMIO" precision="17" />
        </property>
        <property name="imIntereses" type="big_decimal">
            <column name="IM_INTERESES" precision="17" />
        </property>
        <property name="imCostas" type="big_decimal">
            <column name="IM_COSTAS" precision="17" />
        </property>
        <property name="fhActualizacion" type="timestamp">
            <column name="FH_ACTUALIZACION" length="11" />
        </property>
        <property name="coUsuarioActualizacion" type="string">
            <column name="CO_USUARIO_ACTUALIZACION" length="10" />
        </property>
        <many-to-one name="cargoDTO" class="es.dipucadiz.etir.comun.dto.CargoDTO" fetch="select">
            <column name="CO_CARGO" precision="10" scale="0" />
        </many-to-one>     
        <property name="clase" type="string">
            <column name="CLASE" length="3" />
        </property>
       	<property name="tipo" type="string">
            <column name="TIPO" length="1" />
        </property>
        <property name="rowid" type="string" update="false" insert="false">
        	  <column name="ROWID" length="18" />
        </property>
    </class>
</hibernate-mapping>
