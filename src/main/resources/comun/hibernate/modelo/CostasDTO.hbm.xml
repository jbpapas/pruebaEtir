<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="es.dipucadiz.etir.comun.dto.CostasDTO" table="GA_COSTAS">
        <id name="coCostas" type="java.lang.Long">
            <column name="CO_COSTAS" precision="10" scale="0" />
            <generator class="sequence">
                <param name="sequence">SEQ_COSTAS</param>
            </generator>
        </id>
        <many-to-one name="unidadAdministrativaDTO" class="es.dipucadiz.etir.comun.dto.UnidadAdministrativaDTO" fetch="select">
            <column name="CO_UNIDAD_ADMINISTRATIVA" length="4" />
        </many-to-one>
        <many-to-one name="clienteDTO" class="es.dipucadiz.etir.comun.dto.ClienteDTO" fetch="select">
            <column name="CO_CLIENTE" length="10" />
        </many-to-one>
        <many-to-one name="codigoTerritorialDTO" class="es.dipucadiz.etir.comun.dto.CodigoTerritorialDTO" fetch="select">
            <column name="CO_CODIGO_TERRITORIAL" length="6" />
        </many-to-one>
        <many-to-one name="documentoDTO" class="es.dipucadiz.etir.comun.dto.DocumentoDTO" fetch="select">
	        <column name="CO_MODELO" length="3" />
	        <column name="CO_VERSION" length="1" />
	        <column name="CO_DOCUMENTO" length="9" />
        </many-to-one>
        <many-to-one name="documentoCostasDTO" class="es.dipucadiz.etir.comun.dto.DocumentoDTO" fetch="select">
	        <column name="CO_MODELO_COSTAS" length="3" />
	        <column name="CO_VERSION_COSTAS" length="1" />
	        <column name="CO_DOCUMENTO_COSTAS" length="9" />
        </many-to-one>
        <property name="tipo" type="string">
            <column name="TIPO" length="1" />
        </property>
        <property name="descripcion" type="string">
            <column name="DESCRIPCION" length="200" />
        </property>
         <property name="fxAlta" type="date">
            <column name="FX_ALTA" length="7" />
        </property>
        <property name="importe" type="big_decimal">
            <column name="IMPORTE" precision="17" />
        </property>        
        <property name="estado" type="string">
            <column name="ESTADO" length="1" />
        </property>
        <many-to-one name="bdDocumentalGrupoDTO" class="es.dipucadiz.etir.comun.dto.BDDocumentalGrupoDTO" fetch="select">
            <column name="CO_BD_DOCUMENTAL_GRUPO" length="6" />
        </many-to-one>
        <many-to-one name="bienEmbargableDTO" class="es.dipucadiz.etir.comun.dto.BienEmbargableDTO" fetch="select">
            <column name="CO_BIEN_EMBARGABLE" length="10" />
        </many-to-one>
        <property name="nuFactura" type="string">
            <column name="NU_FACTURA" length="12" />
        </property>
         <property name="fhActualizacion" type="timestamp">
            <column name="FH_ACTUALIZACION" length="11" />
        </property>
        <property name="coUsuarioActualizacion" type="string">
            <column name="CO_USUARIO_ACTUALIZACION" length="10" />
        </property>
        <property name="rowid" type="string" update="false" insert="false">
        	  <column name="ROWID" length="18" />
        </property>
	</class>

</hibernate-mapping>