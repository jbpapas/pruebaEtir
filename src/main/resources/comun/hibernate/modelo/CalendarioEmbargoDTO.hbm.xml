<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="es.dipucadiz.etir.comun.dto.CalendarioEmbargoDTO" table="GA_CALENDARIO_EMBARGO">
		<id name="coCalendarioEmbargo" type="java.lang.Long">
			<column name="CO_CALENDARIO_EMBARGO" precision="10" scale="0" />
			<generator class="sequence">
				<param name="sequence">SEQ_CALENDARIO_EMBARGO</param>
			</generator>
		</id>
		<property name="codigo" type="string">
			<column name="CODIGO" length="2" />
		</property>
		<property name="ejercicio" type="short">
			<column name="EJERCICIO" precision="4" scale="0" />
		</property>
		<property name="mes" type="short">
			<column name="MES" precision="2" scale="0" />
		</property>
		<property name="dia" type="short">
			<column name="DIA" precision="2" scale="0" />
		</property>
		<property name="fhActualizacion" type="timestamp">
			<column name="FH_ACTUALIZACION" length="11" />
		</property>
		<property name="coUsuarioActualizacion" type="string">
			<column name="CO_USUARIO_ACTUALIZACION" length="10" />
		</property>
		<property name="rowid" type="string" update="false" insert="false">
			<column name="ROWID" length="18" />
		</property>
	</class>
</hibernate-mapping>